  </servlet>
  <servlet-mapping>
  	<servlet-name>FAQ_write</servlet-name>
  	<url-pattern>/FAQ_write.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>FAQ_list</servlet-name>
